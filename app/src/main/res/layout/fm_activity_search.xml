<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:background="@color/white"
              android:layout_width="match_parent"
              android:layout_height="match_parent">


    <include layout="@layout/fm_search_frame" />


    <fragment
        android:id="@+id/fm_fragment_search"
        android:name="com.fengmap.drpeng.SearchFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <fragment
        android:id="@+id/fm_fragment_search_result"
        android:name="com.fengmap.drpeng.SearchResultFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>